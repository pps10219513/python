pipeline {
    agent any
    stages {
        stage("Checkout") {
            steps {
                git url: 'https://github.com/pps10219513/python', branch: "main"
            }
        }
        stage('Unit Test') {
               steps {
                    sh '''
                    docker  build -t tester .
		    docker run --name test_python tester:latest 
		    docker  rm test_python
		    docker rmi tester
                    '''
               }
        }
    }
}

